{
  "name": "oh-vue-icons",
  "private": true,
  "description": "Include inline SVG icons from different popular icon packs in Vue easily.",
  "license": "MIT",
  "author": "Renovamen <renovamenzxh@gmail.com>",
  "scripts": {
    "icons": "node scripts/build.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "nuxt",
    "build": "nuxt generate",
    "preview": "cd docs/dist && http-server",
    "download": "sh ./scripts/download.sh",
    "lint": "eslint --ext .js,.vue ./",
    "postinstall": "husky install",
    "pub": "sh ./scripts/publish.sh",
    "submodule": "git submodule update --init",
    "update": "sh ./scripts/update.sh",
    "cp-readme": "cp {README.md,README-CN.md} package/"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ext .js,.vue ./"
    ],
    "package.json": [
      "sort-package-json"
    ],
    "package/package.json": [
      "sort-package-json"
    ]
  },
  "devDependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/i18n": "^7.0.3",
    "@nuxtjs/tailwindcss": "4.2.1",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.19.1",
    "glob-promise": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "nuxt": "^2.15.8",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.52.0",
    "svgo": "^1.3.2",
    "vue-slider-component": "^3.2.14",
    "vue-swatches": "^2.1.1"
  }
}
